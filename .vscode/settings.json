    "testMate.cpp.log.logpanel": true,
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "{build,Build,BUILD,out,Out,OUT,builddir}/tests/*{test,Test,TEST}*",
            "cwd": "${absDirpath}",
            "env": {
                "PATH": "${workspaceFolder}/builddir/subprojects/fmt-8.1.1${osPathEnvSep}${workspaceFolder}/bin${osPathEnvSep}${os_env:PATH}"
